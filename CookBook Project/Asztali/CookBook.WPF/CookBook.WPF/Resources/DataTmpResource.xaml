<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:CookBook.WPF.ViewModels"
                    xmlns:view="clr-namespace:CookBook.WPF.Views">
    <DataTemplate DataType="{x:Type vm:MealsViewModel}">
        <view:MealsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UserViewModel}">
        <view:UserView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LoginViewModel}">
        <view:LoginView/>
    </DataTemplate>
</ResourceDictionary>